    "## Charting sample"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d28959c2-e864-4598-9457-611adbf594e9",
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "import pandas as pd\n",
    "import plotly.graph_objects as go\n",
    "import requests\n",
    "import json \n",
    "import plotly.io as pio\n",
    
    "\n",
    "# Set Plotly renderer\n",
    "#pio.renderers.default = \"notebook\"\n",
    "\n",
    #"pio.renderers.default = \"browser\"  \n",  # used when runnning on own pc
    "pio.renderers.default = "colab",  # used when running on Colab
    
    "#\n",
    "# API setup\n",
    "url = \"https://api-nwfp.rothamsted.ac.uk/api/measurements/v2/MeasurementsByTypeID/\"\n",
    "params = {\n",
    "    \"catchmentIds\": \"5,6\",\n",
    "    \"Measurements\": \"19,2,20\",\n",
    "    \"dateFrom\": \"2022-10-28\",\n",
    "    \"dateTo\": \"2022-12-13\"\n",
    "}\n",
    "headers = {\n",
    "    \"Authorization\": f\"Bearer {token}\",  # Make sure `token` is defined\n",
    "    \"Cache-Control\": \"no-cache\"\n",
    "}\n",
    "\n",
    "# Fetch data\n",
    "response = requests.get(url, headers=headers, params=params)\n",
    "if response.status_code != 200:\n",
    "    print(\"Failed:\", response.status_code)\n",
    "    print(response.text)\n",
    "    exit()\n",
    "\n",
    "# Flatten nested data\n",
    "data = response.json()\n",
    "flat_data = []\n",
    "for catchment in data['results']['catchments']:\n",
    "    catchment_id = catchment['Catchment_id']\n",
    "    catchment_name = catchment['name']\n",
    "    for location in catchment.get('locations', []):\n",
    "        location_name = location['name']\n",
    "        for m_type in location.get('types', []):\n",
    "            measurement_type = m_type['type']\n",
    "            unit = m_type.get('unit', '')\n",
    "            for entry in m_type.get('values', []):\n",
    "                flat_data.append({\n",
    "                    'catchmentId': catchment_id,\n",
    "                    'catchmentName': catchment_name,\n",
    "                    'location': location_name,\n",
    "                    'measType': measurement_type,\n",
    "                    'unit': unit,\n",
    "                    'datetime': entry['date'],\n",
    "                    'value': entry['value']\n",
    "                })\n",
    "\n",
    "# Create DataFrame\n",
    "df = pd.DataFrame(flat_data)\n",
    "df['datetime'] = pd.to_datetime(df['datetime'])\n",
    "\n",
    "print(df.head())\n",
    "print(\"Available measurement types:\", df['measType'].unique())\n",
    "\n",
    "# Plotting section â€” for 'flow' type\n",
    "measurement_type = \"flow\"\n",
    "df_catchment5 = df[(df['catchmentId'] == 5) & (df['measType'] == measurement_type)].copy()\n",
    "df_catchment6 = df[(df['catchmentId'] == 6) & (df['measType'] == measurement_type)].copy()\n",
    "\n",
    "print(f\"Catchment 5 rows: {len(df_catchment5)}\")\n",
    "print(f\"Catchment 6 rows: {len(df_catchment6)}\")\n",
    "\n",
    "# Plot if there's data\n",
    "if not df_catchment5.empty or not df_catchment6.empty:\n",
    "    fig = go.Figure()\n",
    "\n",
    "    if not df_catchment5.empty:\n",
    "        fig.add_trace(go.Scatter(\n",
    "            x=df_catchment5['datetime'],\n",
    "            y=df_catchment5['value'],\n",
    "            mode='lines',\n",
    "            name='Catchment 5 (flow)',\n",
    "            line=dict(color='blue')\n",
    "        ))\n",
    "\n",
    "    if not df_catchment6.empty:\n",
    "        fig.add_trace(go.Scatter(\n",
    "            x=df_catchment6['datetime'],\n",
    "            y=df_catchment6['value'],\n",
    "            mode='lines',\n",
    "            name='Catchment 6 (flow)',\n",
    "            line=dict(color='red')\n",
    "        ))\n",
    "\n",
    "    fig.update_layout(\n",
    "        title=f'{measurement_type} Data for Catchments',\n",
    "        xaxis_title='Datetime',\n",
    "        yaxis_title='Value'\n",
    "    )\n",
    "    fig.show()\n",
    "else:\n",
    "    print(\"No data found for measurement type:\", measurement_type)\n",
    "\n"
   ]
